{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "space-infix-ops": ["error", { "int32Hint": false }],
        "indent": ["error", 2],
    "semi": ["error", "always"],
    "quotes": ["error", "single"],"indent": ["error", 2], 
    "semi": ["error", "always"], 
    "quotes": ["error", "single"]
    },
    "settings": {
        "react": {
            "version": "detect"
    }
  }
}
